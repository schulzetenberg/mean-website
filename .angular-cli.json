{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "mean-website-2"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "../node_modules/font-awesome/css/font-awesome.min.css",
        "../node_modules/flexslider/flexslider.css",
        "../node_modules/lightbox2/dist/css/lightbox.min.css",
        "app/shared/css/site-theme.css",
        "app/shared/css/font-cardo.css",
        "app/shared/css/custom.css",
        "app/shared/css/photostack.css",
        "app/shared/css/books.css",
        "app/shared/css/states.css",
        "../node_modules/font-awesome/css/font-awesome.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/q/q.js",
        "../node_modules/lodash/lodash.js",
        "../node_modules/d3/build/d3.min.js",
        "../node_modules/dimple/dist/dimple.latest.js",
        "../node_modules/bootstrap/dist/js/bootstrap.min.js",
        "../node_modules/flexslider/jquery.flexslider-min.js",
        "../node_modules/lightbox2/dist/js/lightbox.min.js",
        "../node_modules/scrollreveal/dist/scrollReveal.min.js",
        "../node_modules/isotope-layout/dist/isotope.pkgd.min.js",
        "../node_modules/typed.js/dist/typed.min.js",
        "../node_modules/twitter-fetcher/js/twitterFetcher_min.js",
        "../node_modules/spectragram/spectragram.js",
        "../node_modules/skrollr/dist/skrollr.min.js",
        "app/shared/js/modernizr.min.js",
        "app/shared/js/photostack.js",
        "app/shared/js/google-analytics.js",
        "app/shared/js/scripts.js",
        "app/shared/js/typing.js"
      ],
      "addons": [
        "../node_modules/font-awesome/fonts/*.+(otf|eot|svg|ttf|woff|woff2)"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "serve": {
      "port": 8999
    },
    "component": {
      "viewEncapsulation": "Emulated",
      "changeDetection": "Default"
    }
  }
}
